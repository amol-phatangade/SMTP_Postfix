
$TTL	604800
@       IN      SOA		mymail.in. admin.mymail.in. (
					  3        ; Serial
					  604800   ; Refresh
					  86400    ; Retry
					  2419200  ; Expire
					  604800 ) ; Negative Cache TTL
; name servers
       IN      NS      ns1.mymail.in.


; PTR Records
10.2    IN      PTR     ns1.mymail.in.    ; 192.168.2.10
11.2    IN      PTR     host1.mymail.in.  ; 192.168.2.11
15.2    IN      PTR     host2.mymail.in.  ; 192.168.2.15
